<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- The app branding color for the app bar -->
    <color name="colorPrimary">#2D3640</color>

    <!-- Darker variant for the status bar and contextual app bars -->
    <color name="colorPrimaryDark">#394450</color>

    <!-- Accent color for UI controls like checkboxes and text fields -->
    <color name="colorAccent">#F0514B</color>

    <!-- Color for the EditText about score team-->
    <color name="colorEditTextScore">@android:color/white</color>

    <!-- Color for the EditText about label team-->
    <color name="colorEditTextTeam">#616161</color>

    <!-- Color for the EditText about card team-->
    <color name="colorEditTextCard">@android:color/black</color>
</resources>
